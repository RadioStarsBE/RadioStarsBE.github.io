---
permalink: /feed-test.atom
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ site.title | xml_escape }}</title>
  <link href="{{ site.url }}{{ site.baseurl }}/feed.xml" rel="self" type="application/atom+xml" title="{{ site.title }} - Atom Feed" />
  <link href="{{ site.url }}/feed.json" rel="alternate" type="application/json" title="{{ site.title }} - JSON Feed" />
  <link href="{{ site.url }}/rss.xml" rel="alternate" type="application/rss+xml" title="{{ site.title }} - RSS 2.0" />
  <link href="{{ site.url }}/feed.rss" rel="alternate" type="application/rss+xml" title="{{ site.title }} - RSS 0.9x" />
  <link href="{{ site.url }}{{ site.baseurl }}" rel="alternate" type="text/html" />
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>{{ site.url }}/</id>
  <author>
    <name>{{ site.author.name | default: site.title | xml_escape }}</name>
  </author>
  <rights>Copyright Â© {{ site.time | date: '%Y' }} {{ site.author.name | default: site.title | xml_escape }}</rights>
  {% for post in site.posts limit:20 %}
  <entry>
    <title>{{ post.title | xml_escape }}</title>
    <link href="{{ site.url }}{{ site.baseurl }}{{ post.url }}" />
    <id>{{ site.url }}{{ site.baseurl }}{{ post.id }}</id>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <summary>{{ post.excerpt | xml_escape }}</summary>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
